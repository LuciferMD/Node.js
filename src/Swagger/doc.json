                        "type":"string",
                        "in":"body"
                    }
                ],
                "produces":"application/json",
    
                "responses":{
                    "200":{"description":"Login", 
                            "schema":{
                                "type": "string",
                                "headers":{
                                    "X-Rate-Limit":{
                                        "type": "integer",
                                        "format": "int32",
                                        "description": "calls per hour allowed by the user"},
                                    "X-Expires-After":{
                                        "type": "string",
                                        "format": "date-time",
